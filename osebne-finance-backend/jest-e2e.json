{
  "testEnvironment": "node",
  "roots": ["<rootDir>/test"],
  "moduleFileExtensions": ["ts", "js", "json"],
  "testRegex": ".*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.ts$": [
      "ts-jest",
      {
        "tsconfig": "tsconfig.json",
        "isolatedModules": true
      }
    ]
  }
  }
